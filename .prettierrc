{
  "$schema": "http://json.schemastore.org/prettierrc",
  "arrowParens": "always",
  "printWidth": 80,
  "quoteProps": "consistent",
  "endOfLine": "lf",
  "semi": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "singleQuote": false,
  "jsxBracketSameLine": false,
  "overrides": [
    {
      "files": [
        "*.json",
        "*.yml",
        "*.md"
      ],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
